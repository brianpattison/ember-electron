{
  "name": "electron",
  "version": "0.1.0",
  "description": "Ember CLI + Electron + Bourbon + Neat",
  "author": {
    "name": "Brian Pattison",
    "email": "brian@brianpattison.com"
  },
  "main": "main.js",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "dist": "mkdir -p ./tmp && rm -rf ./tmp && mkdir -p ./dist && rm -rf ./dist && mkdir ./dist && source .env && ELECTRON_ENV=production && ember build --output-path=./tmp/dist/ && cp main.js ./tmp/dist/main.js && cp package.json ./tmp/dist/package.json && electron-packager ./tmp/dist/ \"$APP_NAME\" --all --app-version=\"$APP_VERSION\" --asar=true --out=./dist --overwrite --version=`npm view electron-prebuilt version` && open ./dist",
    "start": "mkdir -p ./tmp && rm -rf ./tmp && nf start",
    "test": "ember test"
  },
  "repository": "https://github.com/brianpattison/ember-electron",
  "engines": {
    "node": ">= 0.10.0"
  },
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.1.2",
    "electron-packager": "^5.0.2",
    "electron-prebuilt": "^0.31.0",
    "ember-cli": "1.13.8",
    "ember-cli-app-version": "0.5.0",
    "ember-cli-babel": "^5.1.3",
    "ember-cli-bourbon": "1.1.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.0.1",
    "ember-cli-htmlbars": "0.7.9",
    "ember-cli-htmlbars-inline-precompile": "^0.2.0",
    "ember-cli-ic-ajax": "0.2.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-meyer-reset-sass": "0.0.5",
    "ember-cli-neat": "0.0.5",
    "ember-cli-qunit": "^1.0.0",
    "ember-cli-release": "0.2.3",
    "ember-cli-sass": "4.1.0",
    "ember-cli-sri": "^1.0.3",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "2.0.0",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-export-application-global": "^1.0.3",
    "ember-indexeddb-adapter": "0.1.0",
    "foreman": "^1.4.1"
  }
}
